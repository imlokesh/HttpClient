<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	  <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  <Title>IMLokesh.HttpClient</Title>
	  <Authors>imlokesh</Authors>
	  <Description>An http library for dotnet with http2 support and many more features out of the box.  </Description>
	  <Copyright>Copyright Â© 2022</Copyright>
	  <PackageProjectUrl>https://github.com/imlokesh/HttpClient</PackageProjectUrl>
	  <PackageTags>http;httpclient;dotnet;web scraping;</PackageTags>
	  <PackageLicenseExpression>Unlicense</PackageLicenseExpression>
	  <RepositoryUrl>https://github.com/imlokesh/HttpClient</RepositoryUrl>
	  <Version>1.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AngleSharp" Version="0.17.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
